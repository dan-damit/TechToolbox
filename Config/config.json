{
  "paths": {
    "temp": "C:\\Temp\\TechToolbox",
    "logs": "C:\\LogsAndExports\\TechToolbox\\Logs",
    "exportDirectory": "C:\\LogsAndExports\\TechToolbox\\Exports"
  },

  "Dependencies": [
    {
      "Name": "ExchangeOnlineManagement",
      "Version": "3.9.0",
      "Bundled": true,
      "Required": true,
      "Defer": true
    }
  ],

  "settings": {
    "defaults": {
      "promptForHostname": true,
      "promptForCredentials": true,
      "promptForDateRanges": true,
      "promptForCaseName": true,
      "promptForSearchName": true,
      "promptForKqlQuery": true,
      "promptForContentMatchQuery": true,
      "showProgress": true
    },
    "logging": {
      "enableConsole": true,
      "enableFileLogging": true,
      "includeTimestamps": true,
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs",
      "logFileNameFormat": "TechToolbox_{yyyyMMdd}.log",
      "logFile": "TechToolbox.log",
      "minimumLevel": "Info"
    },
    "dnsLogging": {
      "enabled": true,
      "autoEnableDiagnostics": true,
      "parseMode": "simple",
      "maxLogSizeMB": 50,
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\DNS"
    },
    "copyDirectory": {
      "runRemote": true,
      "defaultComputerName": null,
      "logDir": "C:\\LogsAndExports\\TechToolbox\\Logs\\Robocopy",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\Robocopy",
      "logFileNameFormat": "Robocopy_{yyyyMMdd}.log",
      "retryCount": 2,
      "waitSeconds": 5,
      "copyFlags": ["/E", "/COPYALL"],
      "mirror": false
    },
    "subnetScan": {
      "pingTimeoutMs": 250,
      "tcpTimeoutMs": 500,
      "httpTimeoutMs": 1000,
      "ewmaAlpha": 0.15,
      "displayAlpha": 0.1,
      "defaultPort": 80,
      "exportCsv": true,
      "resolveNames": true,
      "httpBanner": true,
      "logFile": "SubnetScan.log",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\SubnetScan",
      "exportDir": "C:\\LogsAndExports\\TechToolbox\\Exports\\SubnetScan"
    },
    "systemRepair": {
      "runRemote": true,
      "defaultComputerName": null,
      "logDir": "C:\\LogsAndExports\\TechToolbox\\Logs\\SystemRepair",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\SystemRepair",
      "logFileNameFormat": "SystemRepair_{yyyyMMdd}.log"
    },
    "windowsUpdate": {
      "logDir": "C:\\LogsAndExports\\TechToolbox\\Logs\\WindowsUpdate",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\WindowsUpdate",
      "logFileNameFormat": "WindowsUpdate_{yyyyMMdd}.log"
    },
    "windowsActivation": {
      "logDir": "C:\\LogsAndExports\\TechToolbox\\Logs\\WindowsActivation",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\WindowsActivation",
      "logFileNameFormat": "ActivationInfo_{computer}_{yyyyMMdd-HHmmss}.txt"
    },
    "output": {
      "writeTable": true,
      "encoding": "UTF8"
    },
    "remoteSoftwareInventory": {
      "includeAppx": false,
      "consolidated": false,
      "throttleLimit": 32,
      "promptForCredentials": true,
      "outDir": "C:\\LogsAndExports\\TechToolbox\\Exports\\RemoteSoftwareInventory",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\RemoteSoftwareInventory",
      "logFileNameFormat": "RemoteSoftwareInventory_{yyyyMMdd}.log",
      "logFile": "RemoteSoftwareInventory.log"
    },
    "compliance": {
      "searchBatchSize": 50,
      "purgeBatchSize": 25,
      "maxRetryAttempts": 3
    },
    "aadSync": {
      "defaultPort": 5985,
      "defaultPolicyType": "Delta",
      "allowKerberos": true,
      "logDir": "C:\\LogsAndExports\\TechToolbox\\Logs\\AADSync",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\AADSync",
      "logFile": "AADSync.log"
    },
    "exchangeOnline": {
      "autoConnect": true,
      "autoDisconnectPrompt": true,
      "showProgress": false
    },
    "messageTrace": {
      "promptForMissingInputs": true,
      "defaultLookbackHours": 48,
      "autoExport": true,
      "timestampFormat": "yyyyMMdd-HHmmss",
      "summaryFileNamePattern": "MessageTraceSummary_{0}.csv",
      "detailsFileNamePattern": "MessageTraceDetails_{0}.csv",
      "defaultExportFolder": "C:\\LogsAndExports\\TechToolbox\\Exports\\MessageTraces"
    },
    "batteryReport": {
      "reportPath": "C:\\Temp\\battery-report.html",
      "outputJson": "C:\\Temp\\installed-batteries.json",
      "debugInfo": "C:\\Temp\\installed-batteries_debug.txt",
      "waitTimeoutSeconds": 10
    },
    "purview": {
      "autoConnect": true,
      "autoDisconnectPrompt": true,
      "defaultAllMailboxes": true,
      "registrationWaitSeconds": 90,
      "registrationPollSeconds": 3,
      "promptForContentMatchQuery": true,
      "search": {
        "maxAttempts": 40,
        "delaySeconds": 10
      },
      "purge": {
        "timeoutSeconds": 1200,
        "pollSeconds": 5,
        "requireConfirmation": true
      }
    },
    "browserCleanup": {
      "killProcesses": true,
      "sleepAfterKillMs": 1500,
      "includeCookies": true,
      "includeCache": true,
      "skipLocalStorage": false,
      "defaultBrowser": "All",
      "defaultProfiles": null,
      "logFile": "BrowserCleanup.log",
      "logPath": "C:\\LogsAndExports\\TechToolbox\\Logs\\BrowserCleanup"
    },
    "pagefile": {
      "defaultPath": "C:\\pagefile.sys",
      "minSizeMB": 16,
      "maxSizeMB": 2097152
    },
    "downloadsCleanup": {
      "cutoffYear": 2024,
      "includeExtensions": null,
      "excludeExtensions": [".pst", ".ost"],
      "dryRun": false
    },
    "tenant": {
      "tenantId": "e1b83792-ab2b-418b-9481-fe12c76f201e",
      "clientId": "9c0e43db-f3fc-4bba-9530-10b5d063730b",
      "organizationName": "valueaddedcompanies.onmicrosoft.com",
      "authorityUrl": "https://login.microsoftonline.com",
      "defaultUsageLocation": "US",
      "defaultMailboxLanguage": "en-US",
      "upnSuffix": "vadtek.com"
    },
    "naming": {
      "upnPattern": "first.last",
      "samPattern": "flast",
      "mailNicknamePattern": "first.last"
    },
    "systemSnapshot": {
      "exportPath": "C:\\LogsAndExports\\TechToolbox\\Exports\\SystemSnapshot",
      "includeRoles": true,
      "includeServices": true
    },
    "licenseMap": {
      "6fd2c87f-b296-42f0-b197-1e91e994b900": "Microsoft 365 E3",
      "c42b9cae-ea4f-4ab7-9717-81576235ccac": "Exchange Online Plan 2",
      "94763226-9b3c-4e75-a931-5c89701abe66": "Teams Phone Standard"
    }
  }
}
